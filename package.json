    "expo": "~44.0.0",
    "expo-font": "~10.0.4",
    "expo-status-bar": "~1.2.0",
    "json-server": "^0.17.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
